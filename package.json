    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.6.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.10",
    "sib-api-v3-sdk": "^8.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
