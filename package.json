  "description": "Restaurant reservation system for Rwanda",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "build:css": "npx tailwindcss -i ./frontend/src/input.css -o ./frontend/public/css/output.css --minify",
    "watch:css": "npx tailwindcss -i ./frontend/src/input.css -o ./frontend/public/css/output.css --watch",
